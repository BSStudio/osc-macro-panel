    <RootNamespace>osc_macro_panel</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>logo.ico</ApplicationIcon>
    <StartupObject>osc_macro_panel.Program</StartupObject>
    <Authors>Komlós Boldizsár</Authors>
    <Company>Komlós Boldizsár &lt;komlos.boldizsar@gmail.com&gt;</Company>
    <Product>OSC Macro Panel</Product>
